{
  "name": "tacticiq",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web --no-dev",
    "web:dev": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "detox:build:ios": "detox build --configuration ios.sim.debug",
    "detox:test:ios": "detox test --configuration ios.sim.debug",
    "detox:build:android": "detox build --configuration android.emu.debug",
    "detox:test:android": "detox test --configuration android.emu.debug",
    "e2e:ios": "npm run detox:build:ios && npm run detox:test:ios",
    "e2e:android": "npm run detox:build:android && npm run detox:test:android",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf node_modules && npm install",
    "nuclear-clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/checkbox": "^0.5.20",
    "@react-native-community/slider": "5.0.1",
    "@react-native-firebase/analytics": "^23.8.0",
    "@react-native-firebase/app": "^23.8.0",
    "@react-native-firebase/auth": "^23.8.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@supabase/supabase-js": "^2.90.0",
    "@tanstack/react-query": "^5.90.16",
    "@types/react": "~19.1.10",
    "expo": "~54.0.31",
    "expo-asset": "~12.0.12",
    "expo-blur": "~15.0.8",
    "expo-build-properties": "~1.0.10",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-router": "~6.0.21",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-iap": "^14.7.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "jest-expo": "^54.0.16"
  },
  "private": true
}
