{
  "name": "tacticiq",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web --no-dev",
    "web:dev": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "detox:build:ios": "detox build --configuration ios.sim.debug",
    "detox:test:ios": "detox test --configuration ios.sim.debug",
    "detox:build:android": "detox build --configuration android.emu.debug",
    "detox:test:android": "detox test --configuration android.emu.debug",
    "e2e:ios": "npm run detox:build:ios && npm run detox:test:ios",
    "e2e:android": "npm run detox:build:android && npm run detox:test:android",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf node_modules && npm install",
    "nuclear-clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@expo/vector-icons": "~14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/checkbox": "^0.5.20",
    "@react-native-community/slider": "4.5.5",
    "@react-native-firebase/analytics": "^23.8.0",
    "@react-native-firebase/app": "^23.8.0",
    "@react-native-firebase/auth": "^23.8.0",
    "@react-navigation/bottom-tabs": "^7.9.0",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "@supabase/supabase-js": "^2.90.0",
    "@tanstack/react-query": "^5.90.16",
    "@types/react": "~18.3.12",
    "expo": "~52.0.0",
    "expo-ads-admob": "^13.0.0",
    "expo-asset": "~11.0.5",
    "expo-blur": "^15.0.8",
    "expo-dev-client": "~5.0.20",
    "expo-file-system": "^19.0.21",
    "expo-font": "~13.0.4",
    "expo-haptics": "^15.0.8",
    "expo-image-picker": "^17.0.10",
    "expo-linear-gradient": "~14.0.2",
    "expo-router": "^3.5.0",
    "expo-status-bar": "~2.0.0",
    "expo-updates": "~0.27.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-iap": "^14.7.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "jest-expo": "^54.0.16"
  },
  "private": true
}
