{
  "name": "tacticiq",
  "version": "1.0.0",
  "main": "index.js",
  "web": "index.web.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web --no-dev",
    "web:dev": "expo start --web",
    "backend:dev": "npm run dev --prefix backend",
    "backend:start": "npm run start --prefix backend",
    "backend:watch": "node scripts/backend-watcher.js",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "workspace:check": "powershell -ExecutionPolicy Bypass -File ./scripts/sync-worktrees.ps1 check",
    "workspace:list": "powershell -ExecutionPolicy Bypass -File ./scripts/sync-worktrees.ps1 list",
    "sync-from-worktree": "powershell -ExecutionPolicy Bypass -File ./scripts/sync-worktrees.ps1 worktree-to-main",
    "sync-to-worktree": "powershell -ExecutionPolicy Bypass -File ./scripts/sync-worktrees.ps1 main-to-worktree",
    "clean": "rm -rf node_modules && npm install",
    "nuclear-clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/checkbox": "^0.5.20",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@supabase/supabase-js": "^2.90.0",
    "@tanstack/react-query": "^5.90.16",
    "@types/react": "~19.1.10",
    "expo": "~54.0.31",
    "expo-asset": "~12.0.12",
    "expo-auth-session": "~7.0.10",
    "expo-av": "~16.0.8",
    "expo-blur": "~15.0.8",
    "expo-build-properties": "~1.0.10",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-localization": "^17.0.8",
    "expo-notifications": "~0.32.16",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "expo-web-browser": "~15.0.10",
    "i18next": "^25.7.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.5.3",
    "react-native": "0.81.5",
    "react-native-iap": "^14.7.1",
    "react-native-reanimated": "^4.2.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets-core": "^1.6.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.28.6",
    "@babel/plugin-transform-template-literals": "^7.27.1"
  },
  "private": true
}
