{
  "expo": {
    "name": "TacticIQ",
    "slug": "tacticiq",
    "version": "1.0.0",
    "scheme": "tacticiq",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#059669"
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true
      },
      "bundleIdentifier": "com.tacticiq.app",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#059669"
      },
      "package": "com.tacticiq.app",
      "userInterfaceStyle": "automatic"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ],
    "extra": {
      "newArchEnabled": true
    },
    "experiments": {
      "typedRoutes": false
    }
  }
}
