{
  "expo": {
    "name": "Fan Manager 2026",
    "slug": "fan-manager-2026",
    "version": "1.0.0",
    "scheme": "fanmanager",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#059669"
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#059669"
      },
      "package": "com.fanmanager.app",
      "userInterfaceStyle": "automatic"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ],
    "extra": {
      "newArchEnabled": true
    },
    "experiments": {
      "typedRoutes": false
    },
    "scheme": "fanmanager"
  }
}
