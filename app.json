{
  "expo": {
    "name": "TacticIQ",
    "slug": "tacticiq",
    "version": "1.0.0",
    "scheme": "tacticiq",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#059669"
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true
      },
      "bundleIdentifier": "com.tacticiq.app",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#059669"
      },
      "package": "com.tacticiq.app",
      "userInterfaceStyle": "automatic"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ],
      "expo-asset",
      "expo-font"
    ],
    "extra": {
      "newArchEnabled": true,
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f33d2180-e311-47ac-b004-59e271e144f4"
      }
    },
    "experiments": {
      "typedRoutes": false
    }
  }
}
